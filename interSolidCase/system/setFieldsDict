FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volScalarFieldValue DT 2.127e-5
);

regions
(
    sphereToCell
    {
        centre (0 0 0.001085);
        radius 0.00106;
        fieldValues
        (
            volScalarFieldValue alpha.water 1
            volScalarFieldValue DT 1.4e-7
        );
    }
 );


