writeInterval   5e-4;

purgeWrite      0;

writeFormat     binary;

writePrecision  10;

writeCompression compressed;

timeFormat      general;

timePrecision   10;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           10;
maxAlphaCo      10;
maxDeltaT       0.0001;

